# Crear una pila vac√≠a
pila = []

# Agregar elementos a la pila
pila.append(1)
pila.append(2)
pila.append(3)

# Imprimir la pila
print("Pila:", pila)

# Sacar elementos de la pila (LIFO)
elemento = pila.pop()
print("Elemento sacado:", elemento)

# Imprimir la pila actualizada
print("Pila actualizada:", pila)

# Crear una lista
lista = [4, 5, 6]

# Agregar elementos a la lista
lista.append(7)
lista.append(8)

# Imprimir la lista
print("Lista:", lista)

# Acceder a un elemento de la lista
print("Primer elemento de la lista:", lista[0])

# Modificar un elemento de la lista
lista[1] = 10
print("Lista modificada:", lista)

# Recorrer la lista
print("Recorriendo la lista:")
for elemento in lista:
    print(elemento)
